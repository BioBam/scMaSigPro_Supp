	number = {5},
	journaltitle = {Bioinformatics},
	author = {Smolander, Johannes and Junttila, Sini and Venäläinen, Mikko S and Elo, Laura L},
	editor = {Mathelier, Anthony},
	urldate = {2023-06-23},
	date = {2022-02-07},
	langid = {english},
	file = {Full Text:/home/priyansh/Zotero/storage/VSHB52Y8/Smolander et al. - 2022 - scShaper an ensemble method for fast and accurate.pdf:application/pdf},
